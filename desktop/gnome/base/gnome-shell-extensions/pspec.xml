        <Summary>A collection of extensions providing additional and optional functionality to GNOME Shell</Summary>
        <Description>gnome-shell-extensions is a collection of extensions providing additional and optional functionality to GNOME Shell.</Description>
        <Archive sha1sum="35bf9caf38f16160a4f970c622c916eba72f42f4" type="tarxz">mirrors://gnome/gnome-shell-extensions/3.9/gnome-shell-extensions-3.9.5.tar.xz</Archive>
        <BuildDependencies>
            <Dependency>gtk3-devel</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>gnome-shell-extensions</Name>
        <RuntimeDependencies>
            <Dependency>gtk3</Dependency>
            <Dependency>gnome-shell</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="doc">/usr/share/doc</Path>
            <Path fileType="data">/usr/share/glib-2.0</Path>
            <Path fileType="data">/usr/share/xsessions</Path>
            <Path fileType="data">/usr/share/gnome-shell</Path>
            <Path fileType="localedata">/usr/share/locale</Path>
            <Path fileType="data">/usr/share/applications</Path>
            <Path fileType="data">/usr/share/gnome-session</Path>
            <Path fileType="data">/usr/share/gnome-shell/extensions/example*/</Path>
        </Files>
    </Package>

